<form method="post" action="{{ path('app_profil_delete', {'id': app.user.id}) }}" onsubmit="return confirm('Are you sure you want to delete your account?');">
<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app.user.id) }}">
<button class="middle-btn-delete">DELETE <i class="fas fa-trash black-trash"></i></button>
</form>  